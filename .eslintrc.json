{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:solid/typescript",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "solid"],
  "rules": {
    "no-console": "off"
  }
}
